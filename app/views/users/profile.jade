extends ../shared/template

block content
  #contain
    #pic
    #name1= user.name
  //- #name2= user.name
  //- #name3= user.name
  .container
      .section.col-md-8
        #about= user.about
        #line
        each activity in activities
          .activity= activity.name
      .side.col-md-4
        #sex= user.sex
        #lookingFor= user.lookingFor
        #race= user.race
        #religion= user.religion
        #bodyType= user.bodyType
        #height= user.height
